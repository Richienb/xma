{
	"name": "xma",
	"version": "1.0.0",
	"description": "Parse xml into json or the inverse.",
	"repository": "https://github.com/Richienb/xma.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"lint": "xo",
		"test": "xo && ava"
	},
	"keywords": [
		"xml",
		"parse",
		"json",
		"build",
		"transform",
		"object",
		"data"
	],
	"dependencies": {
		"is-plain-obj": "^2.1.0",
		"type-fest": "^0.16.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"ava": "^3.9.0",
		"eslint-config-richienb": "^0.4.2",
		"xo": "^0.32.0"
	},
	"xo": {
		"extends": "richienb",
		"rules": {
			"import/no-unresolved": [
				"error",
				{
					"ignore": [
						"type-fest"
					]
				}
			]
		}
	}
}
